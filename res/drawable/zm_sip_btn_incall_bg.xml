<?xml version="1.0" encoding="utf-8"?>
<selector>
    <item n1:state_pressed="true"
      xmlns:n1="http://schemas.android.com/apk/res/android">
        <shape n1:shape="oval">
            <solid n1:color="@color/zm_white" />
        </shape>
    </item>
    <item n2:state_selected="true"
      xmlns:n2="http://schemas.android.com/apk/res/android">
        <shape n2:shape="oval">
            <solid n2:color="@color/zm_white" />
        </shape>
    </item>
    <item n3:state_enabled="false"
      xmlns:n3="http://schemas.android.com/apk/res/android">
        <shape n3:shape="oval">
            <stroke n3:width="1.0dip" n3:color="@color/zm_text_color_ondark_disable" />
            <solid n3:color="@color/zm_transparent" />
        </shape>
    </item>
    <item>
        <shape n4:shape="oval"
          xmlns:n4="http://schemas.android.com/apk/res/android">
            <stroke n4:width="1.0dip" n4:color="@color/zm_white" />
            <solid n4:color="@color/zm_transparent" />
        </shape>
    </item>
</selector>