        <ImageView android:id="@+id/cycle_meeting_check" android:layout_width="@dimen/activity_horizontal_margin" android:layout_height="@dimen/activity_horizontal_margin" android:src="@drawable/check" android:layout_alignParentRight="true" android:layout_centerVertical="true"/>
        <ImageView android:id="@+id/cycle_meeting_checked" android:visibility="gone" android:layout_width="@dimen/activity_horizontal_margin" android:layout_height="@dimen/activity_horizontal_margin" android:src="@drawable/checked" android:layout_alignParentRight="true" android:layout_centerVertical="true"/>
