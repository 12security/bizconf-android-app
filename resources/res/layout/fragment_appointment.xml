<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android" android:orientation="vertical" android:background="?android:attr/colorBackground" android:layout_width="match_parent" android:layout_height="match_parent">
    <include layout="@layout/public_toolbar"/>
    <ScrollView android:layout_width="match_parent" android:layout_height="wrap_content">
        <LinearLayout android:orientation="vertical" android:paddingBottom="15dp" android:layout_width="match_parent" android:layout_height="wrap_content">
            <include layout="@layout/public_weekcalendar"/>
            <include layout="@layout/public_timezone"/>
            <LinearLayout android:orientation="vertical" android:id="@+id/title_appointment_time" android:background="@color/white" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="@dimen/activity_horizontal_margin2">
                <RelativeLayout android:gravity="center_vertical" android:id="@+id/turn_change_name" android:paddingLeft="@dimen/activity_horizontal_margin" android:paddingRight="@dimen/activity_horizontal_margin" android:layout_width="match_parent" android:layout_height="@dimen/activity_vertical_title">
                    <TextView android:textSize="@dimen/textSize28px" android:textColor="@color/fontcolor" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/conference_theme" android:layout_alignParentLeft="false" android:layout_centerVertical="true"/>
                    <ImageView android:id="@+id/turn_change_name_arrow" android:layout_width="@dimen/activity_horizontal_hint" android:layout_height="@dimen/activity_horizontal_hint" android:src="@drawable/icon_arrow_right_big" android:layout_alignParentRight="true" android:layout_centerVertical="true"/>
                    <TextView android:textSize="@dimen/textSize28px" android:textColor="@color/fontcolor" android:ellipsize="end" android:id="@+id/conf_name" android:layout_width="wrap_content" android:layout_height="wrap_content" android:maxEms="15" android:singleLine="true" android:layout_toLeftOf="@+id/turn_change_name_arrow" android:layout_centerVertical="true"/>
                </RelativeLayout>
                <LinearLayout android:gravity="center_vertical" android:orientation="horizontal" android:paddingLeft="@dimen/activity_horizontal_margin" android:paddingRight="@dimen/activity_horizontal_margin" android:layout_width="match_parent" android:layout_height="@dimen/activity_vertical_title">
                    <TextView android:textSize="@dimen/textSize28px" android:textColor="@color/fontcolor" android:layout_gravity="center_vertical" android:layout_width="0dp" android:layout_height="wrap_content" android:text="@string/start" android:layout_weight="1"/>
                    <LinearLayout android:layout_gravity="center" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_weight="2.5">
                        <EditText android:textSize="@dimen/textSize24PX" android:textColor="@color/fontcolor" android:layout_gravity="center" android:id="@+id/editText_date" android:background="@drawable/shape_editext" android:padding="@dimen/activity_nomal_hint" android:focusable="false" android:layout_width="match_parent" android:layout_height="wrap_content" android:editable="false" android:drawableRight="@drawable/icon_arrow_right"/>
                    </LinearLayout>
                    <LinearLayout android:layout_gravity="center" android:paddingLeft="@dimen/activity_nomal_hint" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_weight="1.5">
                        <EditText android:textSize="@dimen/textSize24PX" android:textColor="@color/fontcolor" android:layout_gravity="center" android:id="@+id/editText_time" android:background="@drawable/shape_editext" android:padding="@dimen/activity_nomal_hint" android:focusable="false" android:layout_width="match_parent" android:layout_height="wrap_content" android:editable="false" android:drawableRight="@drawable/icon_arrow_right"/>
                    </LinearLayout>
                </LinearLayout>
                <LinearLayout android:gravity="center_vertical" android:orientation="horizontal" android:paddingLeft="@dimen/activity_horizontal_margin" android:paddingRight="@dimen/activity_horizontal_margin" android:layout_width="match_parent" android:layout_height="@dimen/activity_vertical_title">
                    <TextView android:textSize="@dimen/textSize28px" android:textColor="@color/fontcolor" android:layout_gravity="center_vertical" android:layout_width="0dp" android:layout_height="wrap_content" android:text="@string/duration_time" android:layout_weight="1"/>
                    <LinearLayout android:layout_gravity="center" android:orientation="horizontal" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_weight="4">
                        <EditText android:textSize="@dimen/textSize24PX" android:textColor="@color/fontcolor" android:layout_gravity="center" android:id="@+id/editText_duration_hour" android:background="@drawable/shape_editext" android:padding="@dimen/activity_nomal_hint" android:focusable="false" android:layout_width="@dimen/activity_time_hint" android:layout_height="wrap_content" android:text="00" android:editable="false" android:drawableRight="@drawable/icon_arrow_right"/>
                        <TextView android:textSize="@dimen/textSize24PX" android:textColor="@color/fontcolor" android:layout_gravity="center" android:layout_width="wrap_content" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/activity_nomal_hint" android:text="@string/hour"/>
                        <EditText android:textSize="@dimen/textSize24PX" android:textColor="@color/fontcolor" android:layout_gravity="center" android:id="@+id/editText_duration_minutes" android:background="@drawable/shape_editext" android:padding="@dimen/activity_nomal_hint" android:focusable="false" android:layout_width="@dimen/activity_time_hint" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/activity_nomal_hint" android:text="15" android:editable="false" android:drawableRight="@drawable/icon_arrow_right"/>
                        <TextView android:textSize="@dimen/textSize24PX" android:textColor="@color/fontcolor" android:layout_gravity="center" android:padding="@dimen/activity_nomal_hint" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/min" android:layout_weight="1"/>
                    </LinearLayout>
                </LinearLayout>
            </LinearLayout>
            <LinearLayout android:orientation="vertical" android:background="@color/white" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginTop="@dimen/activity_horizontal_margin2">
                <RelativeLayout android:gravity="center_vertical" android:id="@+id/turn_change_time_zone" android:paddingLeft="@dimen/activity_horizontal_margin" android:paddingRight="@dimen/activity_horizontal_margin" android:layout_width="match_parent" android:layout_height="@dimen/activity_vertical_title">
                    <TextView android:textSize="@dimen/textSize28px" android:textColor="@color/fontcolor" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/time_zone" android:layout_alignParentLeft="false" android:layout_centerVertical="true"/>
                    <ImageView android:id="@+id/turn_change_time_zone_arrow" android:layout_width="@dimen/activity_horizontal_hint" android:layout_height="@dimen/activity_horizontal_hint" android:src="@drawable/icon_arrow_right_big" android:layout_alignParentRight="true" android:layout_centerVertical="true"/>
                    <TextView android:textSize="@dimen/textSize28px" android:textColor="@color/fontcolor" android:ellipsize="end" android:id="@+id/time_zone_name" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/china_standard_time" android:maxEms="12" android:singleLine="true" android:layout_toLeftOf="@+id/turn_change_time_zone_arrow" android:layout_centerHorizontal="true" android:layout_centerVertical="true"/>
                </RelativeLayout>
                <RelativeLayout android:gravity="center_vertical" android:id="@+id/turn_number_time" android:paddingLeft="@dimen/activity_horizontal_margin" android:paddingRight="@dimen/activity_horizontal_margin" android:layout_width="match_parent" android:layout_height="@dimen/activity_vertical_title">
                    <TextView android:textSize="@dimen/textSize28px" android:textColor="@color/fontcolor" android:id="@+id/early_startmeeting" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/early_start_meeting" android:layout_alignParentLeft="false" android:layout_centerVertical="true"/>
                    <ImageView android:id="@+id/image" android:layout_width="@dimen/activity_horizontal_hint" android:layout_height="@dimen/activity_horizontal_hint" android:src="@drawable/icon_arrow_right_big" android:layout_alignParentRight="true" android:layout_centerVertical="true"/>
                    <TextView android:textSize="@dimen/textSize28px" android:textColor="@color/fontcolor" android:ellipsize="end" android:id="@+id/tvearlytime" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="0分钟" android:maxEms="12" android:singleLine="true" android:layout_toLeftOf="@+id/image" android:layout_centerHorizontal="true" android:layout_centerVertical="true"/>
                </RelativeLayout>
                <RelativeLayout android:gravity="center_vertical" android:id="@+id/turn_number_people" android:paddingLeft="@dimen/activity_horizontal_margin" android:paddingRight="@dimen/activity_horizontal_margin" android:clickable="true" android:layout_width="match_parent" android:layout_height="@dimen/activity_vertical_title" android:onClick="onClick">
                    <TextView android:textSize="@dimen/textSize28px" android:textColor="@color/fontcolor" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/number_of_meeting_rooms" android:layout_alignParentLeft="false" android:layout_centerVertical="true"/>
                    <ImageView android:id="@+id/turn_number_people_arrow" android:layout_width="@dimen/activity_horizontal_hint" android:layout_height="@dimen/activity_horizontal_hint" android:src="@drawable/icon_arrow_right_big" android:layout_alignParentRight="true" android:layout_centerVertical="true"/>
                    <TextView android:textSize="@dimen/textSize28px" android:textColor="@color/fontcolor" android:id="@+id/conf_parties" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/number_10" android:layout_toLeftOf="@+id/turn_number_people_arrow" android:layout_centerVertical="true"/>
                </RelativeLayout>
                <RelativeLayout android:gravity="center_vertical" android:id="@+id/time_zone_time_show" android:paddingLeft="@dimen/activity_horizontal_margin" android:paddingRight="@dimen/activity_horizontal_margin" android:visibility="gone" android:layout_width="match_parent" android:layout_height="@dimen/activity_vertical_title">
                    <TextView android:textSize="@dimen/textSize28px" android:textColor="@color/light_gray" android:ellipsize="end" android:gravity="center" android:id="@+id/time_zone_time_result" android:background="@drawable/public_zone_time_show" android:layout_width="match_parent" android:layout_height="match_parent" android:maxEms="15" android:singleLine="true"/>
                </RelativeLayout>
                <RelativeLayout android:id="@+id/turn_cycle_meeting" android:paddingLeft="@dimen/activity_horizontal_margin" android:paddingRight="@dimen/activity_horizontal_margin" android:clickable="true" android:layout_width="match_parent" android:layout_height="@dimen/activity_vertical_title" android:onClick="onClick">
                    <TextView android:textSize="@dimen/textSize28px" android:textColor="@color/fontcolor" android:layout_gravity="center_vertical" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/regular_meeting" android:layout_alignParentLeft="false" android:layout_centerVertical="true"/>
                    <ImageView android:id="@+id/turn_cycle_meeting_carrow" android:layout_width="@dimen/activity_horizontal_hint" android:layout_height="@dimen/activity_horizontal_hint" android:src="@drawable/icon_arrow_right_big" android:layout_alignParentRight="true" android:layout_centerVertical="true"/>
                    <TextView android:textSize="@dimen/textSize28px" android:textColor="@color/fontcolor" android:id="@+id/cycle_meeting_show" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/never" android:layout_toLeftOf="@+id/turn_cycle_meeting_carrow" android:layout_centerVertical="true"/>
                </RelativeLayout>
                <RelativeLayout android:gravity="center_vertical" android:id="@+id/choice_cycleMeeting_time" android:paddingLeft="@dimen/activity_horizontal_margin" android:paddingRight="@dimen/activity_horizontal_margin" android:visibility="gone" android:clickable="true" android:layout_width="match_parent" android:layout_height="@dimen/activity_vertical_title" android:onClick="onClick">
                    <TextView android:textSize="@dimen/textSize28px" android:textColor="@color/fontcolor" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/appointment_cyclemeeting_end_time" android:layout_alignParentLeft="false" android:layout_centerVertical="true"/>
                    <ImageView android:id="@+id/choice_cycleMeeting_time_arrow" android:layout_width="@dimen/activity_horizontal_hint" android:layout_height="@dimen/activity_horizontal_hint" android:src="@drawable/icon_arrow_right_big" android:layout_alignParentRight="true" android:layout_centerVertical="true"/>
                    <TextView android:textSize="@dimen/textSize28px" android:textColor="@color/light_gray" android:layout_gravity="center_vertical" android:id="@+id/cycle_meeting_end_time_show" android:layout_width="wrap_content" android:layout_height="wrap_content" android:text="@string/cycle_meeting_end_time_defualt" android:layout_toLeftOf="@+id/choice_cycleMeeting_time_arrow" android:layout_centerVertical="true"/>
                </RelativeLayout>
            </LinearLayout>
            <include android:layout_width="match_parent" android:layout_height="match_parent" layout="@layout/public_appointment_setting"/>
            <LinearLayout android:gravity="center_horizontal" android:layout_gravity="center_horizontal" android:orientation="horizontal" android:id="@+id/layout_bottom_share" android:paddingLeft="@dimen/activity_public_hint" android:paddingRight="@dimen/activity_public_hint" android:layout_width="match_parent" android:layout_height="@dimen/activity_button_oppointment" android:layout_marginTop="@dimen/activity_public_hint" android:layout_below="@+id/public_setting">
                <LinearLayout android:id="@+id/layout_add_room" android:visibility="gone" android:layout_width="0dp" android:layout_height="wrap_content" android:layout_weight="1">
                    <Button android:textSize="@dimen/textSize24PX" android:textColor="@color/violet" android:id="@+id/turn_add_room" android:background="@drawable/shape_button" android:layout_width="match_parent" android:layout_height="wrap_content" android:text="@string/new_conference_room"/>
                </LinearLayout>
                <LinearLayout android:orientation="vertical" android:layout_width="match_parent" android:layout_height="wrap_content" android:layout_marginLeft="@dimen/activity_horizontal_margin" android:layout_weight="1">
                    <Button android:textSize="@dimen/textSize28px" android:textColor="@color/violet" android:id="@+id/turn_room_list" android:background="@drawable/shape_button" android:visibility="gone" android:layout_width="match_parent" android:layout_height="@dimen/activity_login_button" android:text="@string/enter_the_conference_room_list"/>
                    <Button android:textSize="@dimen/textSize28px" android:textColor="@color/violet" android:id="@+id/btn_meeting_status" android:background="@drawable/shape_button" android:visibility="gone" android:layout_width="match_parent" android:layout_height="@dimen/activity_login_button" android:text="@string/realize_meeting_status"/>
                </LinearLayout>
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
</LinearLayout>
